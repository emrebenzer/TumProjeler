<script>
    $(document).ready(function () {
        var tarih;
        $.ajax({
            url: '/Home/LisansBitisGun',
            type: 'POST',
            //data: JSON.stringify({ ID: D + "-" + "11" + "-" + "0" }),
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                tarih = parseInt(data);
                if (tarih > 60) {
                    $("#dashboard-report-range").removeAttrs("class");
                    $("#dashboard-report-range").attr("class", "pull-right tooltips btn btn-fit-height green");
                    $("#lisansIcerik").html("&nbsp; Lisansınızın bitmesine " + tarih + " gün kalmıştır.");
                }
                else {
                    $("#dashboard-report-range").removeAttrs("class");
                    $("#dashboard-report-range").attr("class", "pull-right tooltips btn btn-fit-height red");
                    $("#lisansIcerik").html("&nbsp; Lisansınızın bitmesine " + tarih + " gün kalmıştır.");
                }
            }
        });

        
        
        
    });

</script>	
