                    $("#PAvergiDahil").html(data.GenelBilgiler.VergiDahil + " €");
                    $("#PATextIskonto").html(data.GenelBilgiler.Komisyon + " €");
                    $("#PAgenelToplam").html(data.GenelBilgiler.GenelToplam + " €");
                    $("#PATextKomisyonOran").html(data.GenelBilgiler.KomisyonOran);
                }

