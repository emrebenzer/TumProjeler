        GenelToplam = VergiDahil - Iskonto;

        $("#toplamAdet").html(ToplamAdet);
        $("#araToplam").html(AraToplam.toFixed(2).replace(".", ",") + " €");
        $("#vergiTutari").html(VergiTutari.toFixed(2).replace(".", ",") + " €");
