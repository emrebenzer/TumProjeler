                    $("#DAVA_GENEL").addClass("start active open");
                } else if (url[5] == "2") {
                    $("#SUC_DUYURU_LI").addClass("start active open");
                }
                else if (url[5] == "1") {
                    $("#ICRA_LI").addClass("start active open");
                }
            }
            else if (url[4] == "YAPILACAK") {
                if (url[5] == "0?modul_ID=2") {
                    $("#DAVA_GENEL").addClass("start active open");
                } else if (url[5] == "0?modul_ID=3") {
                    $("#SUC_DUYURU_LI").addClass("start active open");
                }
                else if (url[5] == "0?modul_ID=1") {
                    $("#ICRA_LI").addClass("start active open");
                }
            }
            else if (url[4] == "GORUSMELER") {
                $("#GORUSMELER_LI").addClass("start active open");
            }
            else if (url[4] == "MUHASEBE_KASA_GENEL") {
                $("#LINK_10").addClass("start active open");
            }
            else if (url[4] == "VEKALETLER") {
                $("#VEKALET_LI").addClass("start active open");
            }
            else if (url[4] == "EDITOR") {
                $("#LINK_12").addClass("start active open");
            }
            else if (url[4] == "AJANDA") {
                $("#LINK_13").addClass("start active open");
            }
            else if (url[4] == "MUHASEBE_KASA_GENEL") {
                $("#LINK_10").addClass("start active open");
            }
            else if (url[4] == "MUHASEBE_MASRAF_AVANS_GENEL") {
                $("#LINK_10").addClass("start active open");
            }
            else if (url[4] == "OFFICE_STORY") {
                if (url[5] == "0") {
                    $("#DAVA_GENEL").addClass("start active open");
                }
                if (url[5] == "2") {
                    $("#SUC_DUYURU_LI").addClass("start active open");
                }
                if (url[5] == "1") {
                    $("#ICRA_LI").addClass("start active open");
                }
            }
        });
        function CONSTRAINT() {
            $.ajax({
                url: '/Home/CONSTRAINT',
                type: 'POST',
                data: JSON.stringify({ ID: "0" }),
                dataType: 'json',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data, function (k, v) {
                        $(("#" + v)).css("display", "block");

                    });
                }


            });

        }
    </script>
    <script type="text/javascript">
        function OnEndCallback(s, e) {
            window.setTimeout(function () {
                if (s.GetItemCount() == 1) {
                    s.SetSelectedIndex(0);
                    s.HideDropDown();
                }
            }, 0);
        }
