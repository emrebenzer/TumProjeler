        /* tried to create a proof of concept fix (like it was created for jQuery.clean before) */
        /* no luck yet - so it's disabled by false */
        if (false && typeof (jQuery) != "undefined") {
            jQuery.prototype.map = jQuery.map;
            jQuery.map = function (elems, callback, arg) {
                var result = jQuery.prototype.map.call(this, elems, callback, arg);
                if (elems && elems.length > 0 && elems[0].nodeName && elems[0].nodeName === "SCRIPT") {
                    for (var i = 0; result && i < result.length; i++) {
                        var script = result[i];
                        if (script && script.id && script.id.indexOf(__aspxStartupScriptPrefix) == 0) {
                            result.splice(i--, 1);
                        }
                    }
                }

                return result;
            };
        }
    </script>
